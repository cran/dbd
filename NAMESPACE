
importFrom("stats", "runif", "var", "optim", "simulate",
           "coef", "lm", "optimHess", "vcov", "logLik","pchisq")
importFrom("graphics", "axis", "legend", "lines", "contour", "persp",
           "plot.default")
S3method(plot,mleDb)
S3method(plot,mleBb)
S3method(vcov,mleDb)
S3method(vcov,mleBb)
S3method(logLik,mleDb)
S3method(logLik,mleBb)
S3method(expValDb,mleDb)
S3method(expValDb,default)
S3method(varDb,mleDb)
S3method(varDb,default)
S3method(expValBb,mleBb)
S3method(expValBb,default)
S3method(varBb,mleBb)
S3method(varBb,default)
S3method(gof,mleDb)
S3method(gof,mleBb)
S3method(simulate,mleDb)
S3method(simulate,mleBb)
S3method(simulate,Dbdpars)
S3method(simulate,Bbdpars)
S3method(mcCovMat,Bbdpars)
S3method(mcCovMat,Dbdpars)
S3method(mcCovMat,default)
S3method(mcCovMat,mleBb)
S3method(mcCovMat,mleDb)


# Export everything.
exportPattern("*")
